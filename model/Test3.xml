<mujoco >
  <include file="scene.xml"/>

  <extension>
    <plugin plugin="mujoco.sdf.sdflib">
      <instance name="sdf">
        <config key="aabb" value="0"/>
      </instance>
    </plugin>
  </extension>
  <statistic meansize=".1"/>

  <size memory="100M"/>

  <asset>
    <mesh file="Turn.stl"/>
  </asset>

  <asset>
    <mesh  file="ThreeWayPass.stl"/>
  </asset>

  <asset>
    <mesh   file="Pad.stl"/>
  </asset>

  <asset>
    <mesh  file="InsideGuide.stl"/>
  </asset>

  <worldbody>

    <body pos="0 11.85 0" name="pad1">
      <geom type="mesh" mesh="Pad" density ="1000" />
      <site name ="exit1" rgba="0 0 1 1" size ="1 1 1" type ="box" pos ="0 -1 0"/>
      <freejoint/>
    </body>

    <body pos="0 15 0" name="passthrough1">
      <geom type="mesh"  mesh="ThreeWayPass" density ="1" />
      <site name ="enter2" rgba="1 0 0 1" size ="1 1 1" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit2" rgba="0 0 1 1" size ="1 1 1" type ="box" pos ="0 4 0"/>
      <freejoint/>
    </body>



    <body pos="0 19.9509 0">
      <geom type="mesh" mesh="InsideGuide" density ="1" rgba ="0.5 0.5 0.5 1 " />
      <site name ="enter3" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit3" rgba="0 0 1 1" size ="1 1 1" type ="box" pos ="0 4 0"/>
      <freejoint/>
    </body>

    <body  pos="0 25 0">
      <geom type="mesh" mesh="ThreeWayPass" rgba ="0.5 0.5 0.5 .5 "/>
      <site name ="enter4" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit4" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 3 0"/>
      <freejoint/>
    </body>


    <body  pos="0 29.9509 0">
      <geom type="mesh" mesh="InsideGuide" rgba ="0.5 0.5 0.5 .5 " />
      <site name ="enter5" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -2 0"/>
      <site name ="exit5" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 2 0"/>
      <freejoint/>
    </body>

    <body pos="0 35 0">
      <geom type="mesh" mesh="ThreeWayPass"  />
      <site name ="enter6" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit6" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 2 0"/>
      <freejoint/>
    </body>

    <body pos="0 39.9509 0">
      <geom type="mesh" mesh="InsideGuide" />
      <site name ="enter7" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit7" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 2 0"/>
      <freejoint/>
    </body>

    <body pos="0 45 0">
      <geom type="mesh" mesh="ThreeWayPass"  />
      <site name ="enter8" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit8" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 2 0"/>
      <freejoint/>
    </body>


    <body  pos="0 49.9509 0">
      <geom type="mesh" mesh="InsideGuide" />
      <site name ="enter9" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit9" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 3 0"/>
    </body>

    <body  pos="0 55 0">
      <geom type="mesh" mesh="ThreeWayPass" />
      <site name ="enter10" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit10" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 3 0"/>
    </body>

    <!--TURN-->
    <body  pos="0 58.151 0" >
      <geom type="mesh" mesh="Pad" euler ="0 0 180"/>
      <site name ="enter11" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -3 0"/>
      <site name ="exit11" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="0 3 0"/>
    </body>

    <body  pos="0 65 0">
      <geom type="mesh" mesh="Turn" euler ="0 0 0"/>
      <site name ="enter12" rgba="1 0 0 1" size ="1.2 1.2 1.2" type ="sphere" pos ="0 -1 0"/>
      <site name ="exit12" rgba=".2 1 1 1" size ="1 1 1" type ="box" pos ="4 0 0"/>
    </body>

    <body>
      <geom type="mesh" mesh="Pad" euler ="0 0 -90" pos="6.8475 65 0"/>
    </body>
    <!--TURN-->

    <body>
      <geom type="mesh" mesh="ThreeWayPass" euler ="0 0 -90"  pos="10 65 0"/>
    </body>
    <body>
      <geom type="mesh" mesh="InsideGuide" euler ="0 0 -90"  pos="14.9509 65 0"/>
    </body>

    <body>
      <geom type="mesh" mesh="ThreeWayPass" euler ="0 0 -90"  pos="20 65 0"/>
    </body>
    <body>
      <geom type="mesh" mesh="InsideGuide" euler ="0 0 -90"  pos="24.9509 65 0"/>
    </body>

    <body>
      <geom type="mesh" mesh="ThreeWayPass" euler ="0 0 -90"  pos="30 65 0"/>
    </body>

    <!--TURN-->
    <body>
      <geom type="mesh" mesh="Pad" euler ="0 0 90"  pos="33.15 65 0"/>
    </body>

    <body>
      <geom type="mesh" mesh="Turn" euler ="0 0 -90" pos="40 65 0"/>
    </body>

    <body>
      <geom type="mesh" mesh="Pad" euler ="0 0 180" pos="40 58.15 0"/>
    </body>
    <!--TURN-->

    

  </worldbody>


  <equality >

    <connect active="true" site1="exit1" site2="enter2"/>
    <connect active="true" site1="exit2" site2="enter3"/>
    <connect active="true" site1="exit3" site2="enter4"/>
    <connect active="true" site1="exit4" site2="enter5"/>
    <connect active="true" site1="exit5" site2="enter6"/>
    <connect active="true" site1="exit6" site2="enter7"/>
    <connect active="true" site1="exit7" site2="enter8"/>
    <connect active="true" site1="exit8" site2="enter9"/>
    <connect active="true" site1="exit9" site2="enter10"/>
    <connect active="true" site1="exit10" site2="enter11"/>
    <connect active="true" site1="exit11" site2="enter12"/>
  </equality>




  
</mujoco>
